<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Perfil</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='main.css'>
    <script src='main.js'></script>
</head>
<body>
    <main>
        <form action="" method="post">
            <% 
                if(usuario){%>
                    usuario.forEach(u => {%>
                        <input type="text" name="userName" placeholder="<%- usuario.userName %>">
                        <input type="password" name="senha" placeholder="<%- usuario.senha %>">
                    <%})
                    <%}
            %>
        </form>
    </main>
</body>
</html>